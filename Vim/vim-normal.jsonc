    {
      // (NOT REVISED) Close editor
      "before": ["<leader>", "w", "w"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      // (NOT REVISED) Close window
      "before": ["<leader>", "W", "W"],
      "after": ["Z", "Q"]
    },

    /*
      8888ba.88ba                    dP                
      88  `8b  `8b                   88                
      88   88   88 .d8888b. 88d888b. 88  .dP  .d8888b. 
      88   88   88 88'  `88 88'  `88 88888"   Y8ooooo. 
      88   88   88 88.  .88 88       88  `8b.       88 
      dP   dP   dP `88888P8 dP       dP   `YP `88888P' 
      */

    {
      // (NOT REVISED) Go to mark
      "before": ["m"],
      "after": ["`"]
    },
    {
      // (NOT REVISED) Set to mark
      "before": ["m", "m"],
      "after": ["m"]
    },

