    {
      "comment": "Move Cursor Strong Up",
      "key": [
        "up"
      ],
      "command": "vim.remap",
      "when": "editorTextFocus && vim.mode != \"Insert\"",
      "after": "2i"
    },
    {
      "comment": "Move Cursor Strong Down",
      "key": [
        "down"
      ],
      "command": "vim.remap",
      "when": "editorTextFocus && vim.mode != \"Insert\"",
      "after": "2k"
    },
    {
      "comment": "Move Cursor Strong Left",
      "key": [
        "left"
      ],
      "command": "vim.remap",
      "when": "editorTextFocus && vim.mode != \"Insert\"",
      "after": "2j"
    },
    {
      "comment": "Move Cursor Strong Right",
      "key": [
        "right"
      ],
      "command": "vim.remap",
      "when": "editorTextFocus && vim.mode != \"Insert\"",
      "after": "2l"
    }
